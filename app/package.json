{
  "name": "edm",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "convert-webp64": "cd ./src/assets/64-64 && find ./ -type f -name '*.png' -exec sh -c 'cwebp -q 75 $1 -o \"${1%.png}.webp\"' _ {} \\;",
    "convert-webp128": "cd ./src/assets/128-128 && find ./ -type f -name '*.png' -exec sh -c 'cwebp -q 75 $1 -o \"${1%.png}.webp\"' _ {} \\;",
    "convert-webp400": "cd ./src/assets/400-500 && find ./ -type f -name '*.jpg' -exec sh -c 'cwebp -q 75 $1 -o \"${1%.jpg}.webp\"' _ {} \\;",
    "start": "webpack-dev-server --config webpack.dev.js --open ",
    "build": "webpack --config webpack.prod.js",
    "electron": "webpack --config webpack.electron.js"
  },
  "dependencies": {
    "materialize-css": "^1.0.0",
    "three": "^0.114.0",
    "tween": "^0.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "app-manifest-webpack-plugin": "^1.2.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-object-to-json-parse": "^0.0.9",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "html-loader-srcset": "^0.5.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "purgecss-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpackbar": "^4.0.0"
  }
}
